{
  "manifest_version": 3,
  "name": "ChatGPT Code/Text Exporter (perc_ldl)",
  "version": "0.5.0",
  "description": "Export code blocks and conversation text as zip files from ChatGPT.",
  "permissions": [
    "activeTab",
    "scripting",
    "downloads"
  ],
  "host_permissions": [
    "https://chatgpt.com/*",
    "https://chat.openai.com/*"
  ],
  "action": {
    "default_title": "Export ChatGPT Code/Text",
    "default_popup": "popup.html"
  }
}
